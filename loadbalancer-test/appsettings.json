{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.Hosting.Lifetime": "Information",
      "loadbalancer_test.Services.LoadBalancerService": "Debug"
    }
  },
  "LoadBalancer": {
    "Listener": {
      "Ip": "0.0.0.0",
      "Port": 9000
    },
    "Backends": [
      { "Host": "127.0.0.1", "Port": 7001 },
      {
        "Host": "127.0.0.1",
        "Port": 7002
      },
      {
        "Host": "127.0.0.1",
        "Port": 7003
      }
    ],
    "HealthCheck": {
      "IntervalSeconds": 3,
      "TimeoutSeconds": 2,
      "FailureThreshold": 2
    },
    "RoutingStrategy": "RoundRobin",
    "Management": {
      "Ip": "127.0.0.1",
      "Port": 9100,
      "SummaryIntervalSeconds": 30
    }
  }
}
